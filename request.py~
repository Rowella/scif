import requests, re, csv
with open("weather.csv",'w') as f:
    t = []
    for i in range(3,10):
        r = requests.get("http://www.bom.gov.au/climate/dwo/20150{}/text/IDCJDW2124.20150{}.csv".format(i,i))
        t.append(r.text)
    for i in range(3):
        r = requests.get("http://www.bom.gov.au/climate/dwo/20151{}/text/IDCJDW2124.20151{}.csv".format(i,i))
        t.append(r.text)
    for i in range(1,3):
        r = requests.get("http://www.bom.gov.au/climate/dwo/20160{}/text/IDCJDW2124.20160{}.csv".format(i,i))
        t.append(r.text)
    for i in range(len(t)):
        n = t[i].split("\r\n")
        n = n[10:]
        for j in range(len(n)):
            n[i] = n[i][1:]
            print(n[i][1:])
        #print(n[10:])
        t[i] = "\r\n".join(n)
    f.write("\r\n".join(t))
